${ac_uA}HAVE_SYS_SYSTEMINFO_H${ac_uB}HAVE_SYS_SYSTEMINFO_H${ac_uC}1${ac_uD}
${ac_eA}HAVE_SYS_SYSTEMINFO_H${ac_eB}HAVE_SYS_SYSTEMINFO_H${ac_eC}1${ac_eD}
