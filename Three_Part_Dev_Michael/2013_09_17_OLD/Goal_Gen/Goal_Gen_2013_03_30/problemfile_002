(defproblem p_some_problem blocks-htn
(
    (arm-empty)
    (block b2)
    (block b3)
    (block b4)
    (block b2)
    (on-table b2)
    (on b3 b2)
    (on b4 b3)
    (clear b4)
    (on-table b2)
    (clear b2)
)
(;Goal Task Network
    (achieve-goals (
(on b2 b3)
)));End of goal task network
)
