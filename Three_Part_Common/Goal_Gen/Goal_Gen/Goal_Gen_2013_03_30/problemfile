(defproblem p_some_problem blocks-htn
(
    (arm-empty)
    (block b1)
    (block b2)
    (block b3)
    (block b4)
    (on-table b1)
    (on b2 b1)
    (on b3 b2)
    (clear b3)
    (on-table b4)
    (clear b4)
)
(;Goal Task Network
    (achieve-goals (
(on b4 b2)
)));End of goal task network
)
