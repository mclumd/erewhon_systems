      (and
          (clear blockB)
          (on blockB blockG)
          (on blockG blockA)
          (on blockA blockH)
          (on blockH blockF)
          (on blockF blockD)
          (on blockD blockC)
          (on blockC blockE)
          (on-table blockE)
))))