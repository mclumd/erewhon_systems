          (arm-empty)
          (clear blockA)
          (on blockA blockG)
          (on blockG blockD)
          (on blockD blockF)
          (on-table blockF)
          (clear blockB)
          (on blockB blockC)
          (on-table blockC)
          (clear blockE)
          (on blockE blockH)
          (on-table blockH)
))
